{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.SemanticKernel": "Debug",
      "Microsoft.SemanticKernel.Agents": "Debug"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "AllowedHosts": "*",
  "FRONTEND_URL": "http://localhost:3001",
  "DetailedErrors": true,
  "AzureAI": {
    "AzureOpenAI": {
      "Endpoint": "${AZURE_OPENAI_ENDPOINT}",
      "ApiKey": "${AZURE_OPENAI_API_KEY}",
      "ChatDeployment": "${AZURE_OPENAI_DEPLOYMENT:gpt-4o}",
      "EmbeddingDeployment": "${AZURE_OPENAI_EMBEDDING_DEPLOYMENT:text-embedding-3-small}",
      "ApiVersion": "${AZURE_OPENAI_API_VERSION:2024-08-01-preview}"
    },
    "AzureAIInference": {
      "Endpoint": "${AZURE_AI_INFERENCE_ENDPOINT:https://models.github.ai/inference}",
      "ApiKey": "${AZURE_AI_INFERENCE_API_KEY}",
      "ModelName": "${AZURE_AI_INFERENCE_MODEL:gpt-4.1}"
    }
  },
  "AgentConfiguration": {
    "DebugAgent": {
      "Name": "DebugAssistant",
      "Instructions": "You are a development AI assistant. Help with coding, debugging, and development questions. Be detailed in your explanations and provide code examples when helpful. You are running in development mode, so be verbose and explain your reasoning.",
      "Description": "Development assistant for debugging and coding help",
      "Provider": "AzureOpenAI",
      "Model": "gpt-4o",
      "Temperature": 0.1,
      "MaxTokens": 2000
    },
    "TestAgent": {
      "Name": "TestHelper",
      "Instructions": "You are a testing specialist. Help developers write unit tests, integration tests, and understand testing best practices. Focus on .NET testing frameworks like xUnit, NUnit, and MSTest.",
      "Description": "Testing specialist for .NET applications",
      "Provider": "AzureOpenAI",
      "Model": "gpt-4o",
      "Temperature": 0.2,
      "MaxTokens": 1500
    }
  },
  "DeveloperSettings": {
    "EnableSwagger": true,
    "EnableCors": true,
    "CorsOrigins": ["http://localhost:3000", "http://localhost:3001", "http://localhost:8080"],
    "EnableDetailedErrors": true
  }
}